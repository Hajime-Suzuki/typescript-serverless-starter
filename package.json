  "author": "Hajime-Suzuki <hajime.doublebass@gmail.com>",
  "license": "MIT",
  "scripts": {
    "deploy:dev": "yarn sls deploy --stage dev",
    "deploy:prod": "yarn sls deploy --stage prod",
    "dev": "yarn sls offline start",
